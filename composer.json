{
  "name": "gsa/data.gov-deployment",
  "type": "project",
  "license": "GPL-3.0+",
  "description": "Data.gov",
  "support": {
    "issues": "https://github.com/gsa/data.gov/issues"
  },
  "require": {
    "php": ">=7.0",
    "composer/installers": "^1.5",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "~5.2.6",
    "oscarotero/env": "^1.0",
    "symfony/yaml": "^3.4.13",
    "phpdocumentor/reflection-common": "^1.0.1",
    "wpackagist-plugin/advanced-custom-fields": "*",
    "wpackagist-plugin/akismet": "*",
    "wpackagist-plugin/amazon-s3-and-cloudfront": "*",
    "wpackagist-plugin/amazon-web-services": "*",
    "wpackagist-plugin/aryo-activity-log": "*",
    "wpackagist-plugin/baw-login-logout-menu": "*",
    "wpackagist-plugin/broken-link-checker": "*",
    "wpackagist-plugin/bulk-move": "*",
    "wpackagist-plugin/category-sticky-post": "*",
    "wpackagist-plugin/contact-form-7": "*",
    "wpackagist-plugin/custom-content-type-manager": "*",
    "wpackagist-plugin/disable-xml-rpc": "*",
    "wpackagist-plugin/flamingo": "*",
    "wpackagist-plugin/google-analyticator": "*",
    "wpackagist-plugin/m-wp-popup": "*",
    "wpackagist-plugin/no-category-base-wpml": "*",
    "wpackagist-plugin/plugin-vulnerabilities": "*",
    "wpackagist-plugin/posts-in-page": "*",
    "wpackagist-plugin/redirection": "*",
    "wpackagist-plugin/si-captcha-for-wordpress": "*",
    "wpackagist-plugin/simple-tooltips": "*",
    "wpackagist-plugin/ssl-insecure-content-fixer": "*",
    "wpackagist-plugin/the-events-calendar": "*",
    "wpackagist-plugin/upload-scanner": "*",
    "wpackagist-plugin/wordpress-importer": "*",
    "wpackagist-plugin/wp-crontrol": "*",
    "wpackagist-plugin/wordpress-seo": "*",
    "wpackagist-plugin/export-all-urls": "*",
    "gsa/arcgis-map": "dev-master",
    "gsa/bwp-external-links": "dev-master",
    "gsa/custom-permalinks": "dev-master",
    "gsa/custom-post-view-generator": "dev-master",
    "gsa/data.gov": "dev-master",
    "gsa/datagov-custom": "dev-master",
    "gsa/inactive-logout": "dev-master",
    "gsa/metric-count": "dev-master",
    "gsa/restrict-categories": "dev-master",
    "gsa/saml-20-single-sign-on": "dev-master",
    "gsa/sticky-posts-in-category": "dev-master",
    "gsa/twitter-posts-to-blog": "dev-master",
    "gsa/twitter-widget-pro": "dev-master",
    "gsa/usa-search": "dev-master",
    "gsa/wp-sitemap-page": "dev-master",
    "gsa/wp-open311": "dev-master",
    "philipashlock/wp-no-taxonomy-base": "dev-master",
    "aws/aws-sdk-php": "^3",
    "psr/log": "^1"
  },
  "require-dev": {
    "doctrine/instantiator": "^1.1.0",
    "phpunit/phpunit": "^7.2.7",
    "squizlabs/php_codesniffer": "^3.3.1",
    "wpackagist-plugin/check-email": "*",
    "wpackagist-plugin/debug-bar": "*",
    "wpackagist-plugin/log-deprecated-notices": "*",
    "wpackagist-plugin/query-monitor": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/data.gov",
      "no-api": true
    },
    {
      "type": "package",
      "package": {
        "name": "philipashlock/wp-no-taxonomy-base",
        "version": "dev-master",
        "type": "wordpress-plugin",
        "source": {
          "url": "https://github.com/philipashlock/wp-no-taxonomy-base.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/arcgis-map",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/bwp-external-links",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/custom-permalinks",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/custom-post-view-generator.git",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/datagov-custom",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/inactive-logout",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/metric-count",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/restrict-categories",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/saml-20-single-sign-on",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/sticky-posts-in-category",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/twitter-posts-to-blog",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/twitter-widget-pro",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/usa-search",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/wp-sitemap-page",
      "no-api": true
    },
    {
      "type": "git",
      "url": "https://github.com/GSA/wp-open311",
      "no-api": true
    },
    {
        "type": "package",
        "package": {
            "name": "wpackagist-plugin/custom-content-type-manager",
            "version": "0.9.8.9",
            "source": {
                "type": "svn",
                "url": "https://plugins.svn.wordpress.org/custom-content-type-manager/",
                "reference": "tags/0.9.8.9"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "name": "wpackagist-plugin/si-captcha-for-wordpress",
            "version": "3.0.3",
            "source": {
                "type": "svn",
                "url": "https://plugins.svn.wordpress.org/si-captcha-for-wordpress/",
                "reference": "tags/3.0.3"
            }
        }
    },
    {
      "type": "package",
      "package": {
        "name": "wpackagist-plugin/export-all-urls",
        "version": "3.5",
        "source": {
          "type": "svn",
          "url": "https://plugins.svn.wordpress.org/export-all-urls/",
          "reference": "tags/3.5"
        }
      }
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "config": {
    "preferred-install": "dist"
  },
  "platform": {
    "php": "7.0"
  }
}
